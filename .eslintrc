{
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "assert": true
  },
  "plugins": ["@typescript-eslint", "react", "promise", "jest"],
  "extends": [
    "plugin:jest/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "camelcase": 0,
    "no-sequences": 0,
    "no-loop-func": 2,
    "require-yield": 2,
    "jest/valid-expect": "off"
  }
}
